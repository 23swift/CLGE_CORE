@{
    Layout = "_Layout";
    string header=ViewBag.header==null?string.Empty:ViewBag.header;
   
}


<div class="card border-light">
  <div class="card-header">
     <div id="" class="row">
           <h4><i id="" class="fa fa-user-cog text-primary"></i> @header</h4>
      </div>
  </div>
  <div class="card-body">
                     <div class="container-fluid">
                      @RenderBody()
                  </div>
      
      </div>
</div>